<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#25949393"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PROCEDURE NAME"
        android:textColor="#000000"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:id="@+id/text"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:layout_below="@+id/text"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:textColor="#c91111"
        android:text="PROVIDER NAME"
        android:id="@+id/diagnosis_textview"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_arrow_bulet_small"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/diagnosis_textview"
        android:id="@+id/relative1_imageview2"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Average Medicare Amount"
        android:layout_marginTop="6dp"
        android:layout_below="@+id/diagnosis_textview"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        android:textSize="12sp"
        android:layout_toRightOf="@+id/relative1_imageview2"
        android:id="@+id/avg_textview"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_info_small"
        android:layout_toRightOf="@+id/avg_textview"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="4dp"
        android:visibility="invisible"
        android:id="@+id/relative1_imageview3"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$12,234.12"
        android:textColor="#000000"
        android:textSize="12sp"
        android:layout_marginTop="6dp"
        android:id="@+id/textView2"
        android:layout_toRightOf="@+id/relative1_imageview3"
        android:layout_below="@+id/diagnosis_textview"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relative1_imageview2"
        android:src="@drawable/icon_arrow_bulet_small"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="14dp"
        android:id="@+id/relative1_imageview4"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:text="Average Charge Amount"
        android:textColor="#000000"
        android:layout_below="@+id/avg_textview"
        android:layout_toRightOf="@+id/relative1_imageview4"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/total_textview"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_info_small"
        android:layout_toRightOf="@+id/avg_textview"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="4dp"
        android:layout_below="@+id/avg_textview"
        android:id="@+id/relative1_imagevie"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:text="$23,345.23"
        android:id="@+id/relative_textview2"
        android:textColor="#000000"
        android:layout_above="@+id/relative1_imageview6"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relative1_imageview4"
        android:src="@drawable/icon_arrow_bulet_small"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/relative1_imageview5"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Average Medicare Payment"
        android:layout_below="@+id/total_textview"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/relative1_imageview5"
        android:textSize="12sp"
        android:id="@+id/med_textview"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_info_small"
        android:layout_below="@+id/total_textview"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@+id/med_textview"
        android:id="@+id/relative1_imageview6"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:textColor="#000000"
        android:text="$45,567.54"
        android:layout_marginTop="10dp"
        android:id="@+id/textView3"
        android:layout_below="@+id/relative_textview2"
        android:layout_alignLeft="@+id/relative_textview2"
        android:layout_alignStart="@+id/relative_textview2" />

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="5dp"
        android:background="#ffffff"
        android:layout_below="@+id/relative1_imageview6"></View>
</RelativeLayout>