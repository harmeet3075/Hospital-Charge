<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
android:layout_width="match_parent"
android:layout_height="wrap_content"
xmlns:android="http://schemas.android.com/apk/res/android"
android:background="#25949393"
>


<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/icon_arrow_bulet_small"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="14dp"
    android:id="@+id/relative1_imageview1"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:textColor="#000000"
        android:layout_toRightOf="@+id/relative1_imageview1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:text="Discharges"
        android:id="@+id/discharges_textview"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="11"
        android:textSize="12sp"
        android:textColor="#000000"
    android:id="@+id/relative_textview1"
    android:layout_toRightOf="@+id/discharges_textview"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="120dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_arrow_bulet_small"
        android:layout_below="@+id/relative1_imageview1"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/relative1_imageview2"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Average Covered Charges"
        android:layout_marginTop="6dp"
        android:textColor="#000000"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:layout_toRightOf="@+id/relative1_imageview2"
        android:layout_below="@+id/discharges_textview"
        android:id="@+id/avg_textview"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_info_small"
        android:layout_toRightOf="@+id/avg_textview"
        android:layout_below="@+id/discharges_textview"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="4dp"
        android:id="@+id/relative1_imageview3"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$12,234.12"
        android:textSize="12sp"
        android:layout_marginTop="7dp"
        android:textColor="#000000"
        android:id="@+id/avgCharges"
        android:layout_alignBottom="@+id/relative1_imageview3"
        android:layout_alignLeft="@+id/relative_textview1"
        android:layout_alignStart="@+id/relative_textview1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relative1_imageview3"
        android:src="@drawable/icon_arrow_bulet_small"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="14dp"
        android:id="@+id/relative1_imageview4"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:text="Average Total Payment"
        android:layout_below="@+id/avg_textview"
        android:layout_toRightOf="@+id/relative1_imageview4"
        android:layout_marginTop="10dp"
        android:textColor="#000000"
        android:layout_marginLeft="10dp"
        android:id="@+id/total_textview"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:layout_below="@+id/avg_textview"
        android:layout_toRightOf="@+id/total_textview"
        android:text="$23,345.23"
        android:textColor="#000000"
        android:layout_alignStart="@+id/relative_textview1"
        android:layout_alignLeft="@+id/relative_textview1"
        android:layout_marginTop="10dp"
        android:id="@+id/relative_textview2"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relative1_imageview4"
        android:src="@drawable/icon_arrow_bulet_small"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/relative1_imageview5"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Average Medicare Payment"
        android:layout_below="@+id/total_textview"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/relative1_imageview5"
        android:textSize="12sp"
        android:textColor="#000000"
        android:id="@+id/med_textview"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_info_small"
        android:layout_below="@+id/total_textview"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@+id/med_textview"
        android:id="@+id/relative1_imageview6"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:text="$45,567.54"
        android:id="@+id/amp"
        android:textColor="#000000"
        android:layout_below="@+id/relative1_imageview4"
        android:layout_toRightOf="@+id/relative1_imageview6"
        android:layout_marginTop="10dp"
        android:layout_alignStart="@+id/relative_textview1"
        android:layout_alignLeft="@+id/relative_textview1"/>
</RelativeLayout>