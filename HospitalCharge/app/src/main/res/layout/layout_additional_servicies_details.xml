<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_add_servicies_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/header"></include>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#c91111"
        android:text="Hospital Name"
        android:textSize="16sp"
        android:layout_below="@+id/header"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        android:id="@+id/search_textview"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/search_textview"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:text="Address:"
        android:id="@+id/address"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/search_textview"
        android:layout_toRightOf="@+id/address"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:textColor="#000000"
        android:id="@+id/textview1"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview1"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="6dp"
        android:textSize="12sp"
        android:text="Phone:"
        android:id="@+id/phone"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview1"
        android:layout_toRightOf="@+id/phone"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:layout_marginTop="6dp"
        android:textColor="#000000"
        android:id="@+id/textview2"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview2"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="6dp"
        android:textSize="12sp"
        android:text="Zip:"
        android:id="@+id/zip"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview2"
        android:layout_toRightOf="@+id/zip"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:layout_marginTop="6dp"
        android:textColor="#000000"
        android:id="@+id/textview3"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview3"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="6dp"
        android:textSize="12sp"
        android:text="Email:"
        android:id="@+id/email"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview3"
        android:layout_toRightOf="@+id/email"
        android:layout_marginLeft="10dp"
        android:textSize="12sp"
        android:layout_marginTop="6dp"
        android:textColor="#000000"
        android:id="@+id/textview4"
        android:text="NA"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/email"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="4dp"
        android:textSize="16sp"
        android:background="@drawable/btn_click"
        android:text="Click here to see location on map."
        android:textColor="#c91111"
        android:id="@+id/map"></TextView>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#c91111"
        android:layout_below="@+id/map"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="4dp"
        android:text="Claim this listing"
        android:padding="10dp"
        android:textColor="#ffffff"
        android:id="@+id/claim"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/claim"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="3dp"
        android:textSize="16sp"
        android:layout_centerInParent="true"
        android:text="Diagnosis List"
        android:textColor="#c91111"
        android:id="@+id/diaglist"></TextView>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/search_listview"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_below="@+id/diaglist">
    </ListView>
</RelativeLayout>
